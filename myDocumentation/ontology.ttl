@prefix : <http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25> .

<http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#BelongsTo
:BelongsTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :Comprise ;
           rdfs:domain :AbstractConcept ;
           rdfs:range :a-cd:CulturalScope ;
           rdfs:comment "This property link and abstract concept to its origin movement. Example: Teoria dei moti dell'animo was formalised during Renaissance and belongs to it." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Comprise
:Comprise rdf:type owl:ObjectProperty ;
          rdfs:domain :a-cd:CulturalScope ;
          rdfs:range :AbstractConcept ;
          rdfs:comment "Inverse of BelongsTo" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#CompriseIconography
:CompriseIconography rdf:type owl:ObjectProperty ;
                     owl:inverseOf :IconographyCompriseIn ;
                     rdfs:domain :IconographicMotifs ;
                     rdfs:range :IconographicRepresentation ;
                     rdfs:comment "This property link the Iconographical motifs to all its known Iconographical Representations. Example: Last Supper Iconographical Motifs is linked to Florentine Last Supper, Da Vinci Last Supper, Nordic Last Supper ecc..." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#CreatedBy
:CreatedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :IntroductedBy ;
           owl:inverseOf :Creates ;
           rdf:type owl:AsymmetricProperty ;
           rdfs:domain :AbstractConcept ;
           rdfs:range :core:Agent ;
           rdfs:comment "This property highlights the role of creator of an agent of an abstract concept." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Creates
:Creates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :Introduce ;
         rdfs:domain :core:Agent ;
         rdfs:range :AbstractConcept ;
         rdfs:comment "Inverse of CreatedBy" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#DerivativeVisualAtom
:DerivativeVisualAtom rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :HasVisualAtom ;
                      owl:inverseOf :IsDerivativeVisualAtomOf ;
                      rdfs:domain :DerivationRelation ;
                      rdfs:range :VisualAtom ;
                      rdfs:comment "Specialisation of VisualAtom. Identifying the Derivated Visual Atom of a Derivation Relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#DescribedIn
:DescribedIn rdf:type owl:ObjectProperty ;
             rdfs:domain <https://w3id.org/arco/ontology/core/Concept> ;
             rdfs:comment "This property links a concept to a source that describes it" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#EmbodiesArtisticTheory
:EmbodiesArtisticTheory rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :IsPresentIn ;
                        owl:inverseOf :PresentsArtisticTheory ;
                        rdfs:domain :IconographicRepresentation ;
                        rdfs:range :ArtisticTheory ;
                        rdfs:comment "This property link an Iconographical Representation to the Theory it embodies inside it. Da Vinci Last Supper Iconography embodies Theory of Moti dell'Animo inside it." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#FinalVisualAtom
:FinalVisualAtom rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :HasVisualAtom ;
                 owl:inverseOf :IsFinalVisualAtomOf ;
                 rdfs:domain :PreparatoryRelation ;
                 rdfs:range :VisualAtom ;
                 rdfs:comment "Specialisation of VisualAtom. Identifying the Final Visual Atom of a Preparatory relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasAlternativeDating
:HasAlternativeDating rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :Arco:HasDating ;
                      owl:inverseOf :IsAlternativeDatingOf ;
                      rdfs:comment "Specialisation of father property linking an element to its possible alternative dating." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasCharacterVisualAtom
:HasCharacterVisualAtom rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :HasVisualAtom ;
                        rdfs:domain owl:Thing ;
                        rdfs:range :CharacterVisualAtom ;
                        rdfs:comment "Property specialisation of HasVisualAtom, linking an element to a Visual Atom identified as a character." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasCompositionVisualAtom
:HasCompositionVisualAtom rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :HasVisualAtom ;
                          rdfs:domain owl:Thing ;
                          rdfs:range :CompositionVisualAtom ;
                          rdfs:comment "Specialisation of HasVisualAtom used to link an element to a visual Atom representing the composition (the order of figures and elements) of something. Eg: A photo of Last Supper marked in order to highlights the groupings of figures." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasDepiction
:HasDepiction rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( <http://dati.beniculturali.it/cis/CulturalEntity>
                                          :VisualAtom
                                          :core:Agent
                                        )
                          ] ;
              rdfs:range <http://xmlns.com/foaf/spec/#term_Image> .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasDerivatedCulturalProperty
:HasDerivatedCulturalProperty rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                              owl:inverseOf :IsDerivationInRelation ;
                              rdfs:domain :DerivationRelation ;
                              rdfs:range :Arco:CulturalProperty ;
                              rdfs:comment "Inverse of  IsDerivationInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasFinalCulturalProperty
:HasFinalCulturalProperty rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                          owl:inverseOf :IsFinalWorkInRelation ;
                          rdfs:domain :PreparatoryRelation ;
                          rdfs:range :Arco:CulturalProperty .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasGeometricalVisualAtom
:HasGeometricalVisualAtom rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :HasVisualAtom ;
                          rdfs:domain owl:Thing ;
                          rdfs:range :GeometricalVisualAtom ;
                          rdfs:label "Specialisation of HasVisualAtom. It link something to a visual Atom showing the geometrical elements of a Cultural Property. Eg: a photo highlighting the perspectical lines of Leonardo's Last Supper" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasIconographicalMotif
:HasIconographicalMotif rdf:type owl:ObjectProperty ;
                        owl:inverseOf :RepresentSubject ;
                        rdfs:domain :a-cd:Subject ;
                        rdfs:range :IconographicMotifs ;
                        rdfs:comment "Inverse of RepresentSubject" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasIconographicalRepresentation
:HasIconographicalRepresentation rdf:type owl:ObjectProperty ;
                                 owl:inverseOf :IsUsedInWork ;
                                 rdfs:domain :Arco:CulturalProperty ;
                                 rdfs:range :IconographicRepresentation ;
                                 rdfs:comment "This property link a Cultura Property to its iconographical representation. Ex: Cenacolo of Fuligno has a Florentine Last Supper Iconography." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasInfluenceMedium
:HasInfluenceMedium rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                    owl:inverseOf :IsMediumInRelation ;
                    rdfs:comment "Inverse of IsMediumInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasInfluencedCulturalProperty
:HasInfluencedCulturalProperty rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                               owl:inverseOf :IsInfluencedInRelation ;
                               rdfs:domain :InfluenceRelation ;
                               rdfs:range :Arco:CulturalProperty ;
                               rdfs:comment "Inverse of IsInfluencedInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasInfluencerCulturalProperty
:HasInfluencerCulturalProperty rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                               owl:inverseOf :IsInfluencerInRelation ;
                               rdfs:domain :InfluenceRelation ;
                               rdfs:range :Arco:CulturalProperty ;
                               rdfs:comment "Inverse of IsInfluencerInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasInterpretationCriterion
:HasInterpretationCriterion rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain :a-cd:InterpretationCriterion ;
                            rdfs:range :a-cd:InterpretationCriterion .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasOriginalCulturalProperty
:HasOriginalCulturalProperty rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                             owl:inverseOf :IsOriginalInRelation ;
                             rdfs:domain :DerivationRelation ;
                             rdfs:range :Arco:CulturalProperty ;
                             rdfs:comment "Inverse of  IsOriginalInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasPictorialAttribute
:HasPictorialAttribute rdf:type owl:ObjectProperty ;
                       owl:inverseOf :IsAttributeOf ;
                       rdfs:domain :IconographicRepresentation ;
                       rdfs:range :PictorialAttribute ;
                       rdfs:comment "This property link an iconographical representation to a pictorial attribute part of it. eg: Last Supper has the figure of Christ inside it." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasPreparatoryCulturalPropety
:HasPreparatoryCulturalPropety rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :IsRelatedWorkSituationOf ;
                               owl:inverseOf :IsPreparatoryWorkInRelation ;
                               rdfs:domain :PreparatoryRelation ;
                               rdfs:range :Arco:CulturalProperty ;
                               rdfs:comment "Inverse of IsPreparatoryWorkInRelation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#HasVisualAtom
:HasVisualAtom rdf:type owl:ObjectProperty ;
               owl:inverseOf :IsVisualAtomOf ;
               rdfs:domain owl:Thing ;
               rdfs:range :VisualAtom ;
               rdfs:comment "Property linking an Owl:thing to a Visual Atom representing a significant visual feature of it. Eg: Da Vinci's Last Supper Iconography linked to a Visual Atom showing a photo of a representation identifiable as part of this Iconography" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IconographyCompriseIn
:IconographyCompriseIn rdf:type owl:ObjectProperty ;
                       rdfs:domain :IconographicRepresentation ;
                       rdfs:range :IconographicMotifs ;
                       rdfs:comment "Inverse of CompriseIconography" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Influenced
:Influenced rdf:type owl:ObjectProperty ;
            rdfs:domain :ArtisticTheory ;
            rdfs:range :IconographicRepresentation ;
            rdfs:comment "This property highlights the influence that an artistic Theory has on an iconographical representation. At the moment no reification is provided." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#InfluencedVisualAtom
:InfluencedVisualAtom rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :HasVisualAtom ;
                      rdfs:comment "Property used to describe the VisualAtom of the Influenced cultural property." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#InfluencerVisualAtom
:InfluencerVisualAtom rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :HasVisualAtom ;
                      rdfs:comment "Property used to describe the VisualAtom of the Influencer cultural property." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Introduce
:Introduce rdf:type owl:ObjectProperty ;
           owl:inverseOf :IntroductedBy ;
           rdfs:comment "Inverse of IntroducedBy" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IntroductedBy
:IntroductedBy rdf:type owl:ObjectProperty ;
               rdfs:label "" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsAlternativeDatingOf
:IsAlternativeDatingOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :Arco:IsDatingOf ;
                       rdfs:domain :a-cd:Dating ;
                       rdfs:range owl:Thing .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsAttributeOf
:IsAttributeOf rdf:type owl:ObjectProperty ;
               rdfs:domain :PictorialAttribute ;
               rdfs:range :IconographicRepresentation ;
               rdfs:comment "Inverse of  HasAttribute" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsCharacterVisualAtomOf
:IsCharacterVisualAtomOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :IsVisualAtomOf .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsCompositionVisualAtomOf
:IsCompositionVisualAtomOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :IsVisualAtomOf .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsDerivationInRelation
:IsDerivationInRelation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                        rdfs:domain :Arco:CulturalProperty ;
                        rdfs:range :DerivationRelation ;
                        rdfs:comment "Coupled with IsOriginalRelation this property link a cultural Property to the DerivationRelation node. Specifically this property link the derivation one of the 2 Cultural Properties." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsDerivativeVisualAtomOf
:IsDerivativeVisualAtomOf rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :IsVisualAtomOf ;
                          rdfs:domain :VisualAtom ;
                          rdfs:range :DerivationRelation ;
                          rdfs:comment "Inverse of DerivativeVisualAtom" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsFinalVisualAtomOf
:IsFinalVisualAtomOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :IsVisualAtomOf ;
                     rdfs:domain :VisualAtom ;
                     rdfs:range :PreparatoryRelation ;
                     rdfs:comment "Inverse of FinalVisualAtom" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsFinalWorkInRelation
:IsFinalWorkInRelation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                       rdfs:domain :Arco:CulturalProperty ;
                       rdfs:range :PreparatoryRelation ;
                       rdfs:comment "Coupled with IsPreparatoryWorkInRelation. This property link a Cultural Property to its PreparatoryRelation node. Specifically this property identify the final work of the relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsGeometricalVisualAtomOf
:IsGeometricalVisualAtomOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :IsVisualAtomOf .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsIconologicalMeaningOf
:IsIconologicalMeaningOf rdf:type owl:ObjectProperty ;
                         rdfs:range :IconographicRepresentation ;
                         rdfs:comment "Inverse of HasIconologicalMeaning" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsInfluencedInRelation
:IsInfluencedInRelation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                        rdfs:domain :Arco:CulturalProperty ;
                        rdfs:range :InfluenceRelation ;
                        rdfs:comment "Coupled with InfluencerInRelation. This property links a Cultural Property to its influence relation node. Specifically this property identify the cultural property that is influenced by another one." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsInfluencedVisualAtomOf
:IsInfluencedVisualAtomOf rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :IsVisualAtomOf .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsInfluencerInRelation
:IsInfluencerInRelation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                        rdfs:domain :Arco:CulturalProperty ;
                        rdfs:range :InfluenceRelation ;
                        rdfs:comment "Coupled with InfluencerInRelation. This property links a Cultural Property to its influence relation node. Specifically this property identify the cultural property that influence the other one." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsInfluencerVisualAtomOf
:IsInfluencerVisualAtomOf rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :IsVisualAtomOf .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsMediumInRelation
:IsMediumInRelation rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                    rdfs:comment "This property highlights the role of medium in the relation between 2 cultural properties. Eg: The incision from Pietro Birago of Last Supper was the medium of influence from Last Supper to the Flanders." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsOriginalInRelation
:IsOriginalInRelation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                      rdfs:domain :Arco:CulturalProperty ;
                      rdfs:range :DerivationRelation ;
                      rdfs:comment "Coupled with IsOriginalRelation this property link a cultural Property to the DerivationRelation node. Specifically this property link the original one of the 2 cultural properties." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsOriginalVisualAtomOf
:IsOriginalVisualAtomOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :IsVisualAtomOf ;
                        owl:inverseOf :OriginalVisualAtom ;
                        rdfs:domain :VisualAtom ;
                        rdfs:range :DerivationRelation ;
                        rdfs:comment "Inverse of OriginalVisualAtom" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsPredecessorOf
:IsPredecessorOf rdf:type owl:ObjectProperty ;
                 owl:inverseOf :IsSuccessorOf ;
                 rdfs:domain :IconographicRepresentation ;
                 rdfs:range :IconographicRepresentation ;
                 rdfs:comment "This property link an iconographical representation to another one that come immediately before it. Eg: The Florentine iconographical representation is the predecessor of Da Vinci's one." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsPreparatoryVisualAtomOf
:IsPreparatoryVisualAtomOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :IsVisualAtomOf ;
                           owl:inverseOf :PreparatoryVisualAtom ;
                           rdfs:domain :VisualAtom ;
                           rdfs:range :PreparatoryRelation ;
                           rdfs:comment "Inverse of PreparatoryVisualAtom" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsPreparatoryWorkInRelation
:IsPreparatoryWorkInRelation rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :Arco:RelatedWorkSituation ;
                             rdfs:domain :Arco:CulturalProperty ;
                             rdfs:range :PreparatoryRelation ;
                             rdfs:comment "Coupled with IsPreparatoryWorkInRelation. This property link a Cultural Property to its PreparatoryRelation node. Specifically this property identify the preparatory work of the relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsPresentIn
:IsPresentIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :Presents ;
             rdfs:domain :AbstractConcept ;
             rdfs:range :Arco:CulturalProperty ;
             rdfs:comment "It highlights the presence of an abstract concept inside a Cultural property. Eg: Da Vinci's iconographical rapresentation of Last Supper is present in a set of different Cultural Properties. Teoria dei Moti dell'animo is present in another set of other Cultural Properties." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsRelatedWorkSituationOf
:IsRelatedWorkSituationOf rdf:type owl:ObjectProperty ;
                          owl:inverseOf :Arco:RelatedWorkSituation ;
                          rdfs:domain :a-cd:RelatedWorkSituation ;
                          rdfs:range :Arco:CulturalProperty .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsSuccessorOf
:IsSuccessorOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :IconographicRepresentation ;
               rdfs:range :IconographicRepresentation ;
               rdfs:comment "Inverse of IsPredecessorOf" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsUsedInWork
:IsUsedInWork rdf:type owl:ObjectProperty ;
              rdfs:domain :IconographicRepresentation ;
              rdfs:range :Arco:CulturalProperty ;
              rdfs:comment "Inverse of HasIconographicalRepresentation" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#IsVisualAtomOf
:IsVisualAtomOf rdf:type owl:ObjectProperty ;
                rdfs:comment "Inverse of HasVisualAtom" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#OriginalVisualAtom
:OriginalVisualAtom rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :HasVisualAtom ;
                    rdfs:domain :DerivationRelation ;
                    rdfs:range :VisualAtom ;
                    rdfs:comment "Specialisation of VisualAtom. Identifying the Original Visual Atom of the relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#PreparatoryVisualAtom
:PreparatoryVisualAtom rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :HasVisualAtom ;
                       rdfs:domain :PreparatoryRelation ;
                       rdfs:range :VisualAtom ;
                       rdfs:comment "Specialisation of VisualAtom. Identifying the Preparatory Visual Atom of a Preparatory Relation." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Presents
:Presents rdf:type owl:ObjectProperty ;
          rdfs:domain :Arco:CulturalProperty ;
          rdfs:range :AbstractConcept ;
          rdfs:comment "Inverse of IsPresentIn" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#PresentsArtisticTheory
:PresentsArtisticTheory rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :Presents .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#RepresentSubject
:RepresentSubject rdf:type owl:ObjectProperty ;
                  rdfs:domain :IconographicMotifs ;
                  rdfs:range :a-cd:Subject ;
                  rdfs:comment "This property show the subject highlighted by the Iconographical Motifs" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#TheorisedBy
:TheorisedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :IntroductedBy ;
             owl:inverseOf :Theorises ;
             rdfs:domain :AbstractConcept ;
             rdfs:range :core:Agent ;
             rdfs:comment "This property highlights who theorised a certain artistic theory." .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Theorises
:Theorises rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :Introduce ;
           rdfs:domain :core:Agent ;
           rdfs:range :AbstractConcept ;
           rdfs:comment "Inverse of TheorisedBy" .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Arco:HasAuthor
:Arco:HasAuthor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :Arco:IsAuthorOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://dati.beniculturali.it/cis/CulturalEntity>
                                            <https://w3id.org/arco/ontology/context-description/Bibliography>
                                          )
                            ] ;
                rdfs:range :core:Agent .


###  http://www.semanticweb.org/mattia/ontologies/2020/10/untitled-ontology-25#Arco:HasDating
